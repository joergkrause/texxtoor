@model Texxtoor.DataModels.Models.Users.AddressBook
<div class="box box-padding">
  <form class="box-content" id="addAddressForm">
    @Html.HiddenFor(m => m.Id)
    @using (Html.BtFieldset(Loc("legend", @"Add Address"))) {
      @Html.EditorForModel()
      @Button.FormSendButtons("editAddressSendButton", ViewResources.Action_Profile_EditAddress, "editAddressCancelButton", ViewResources.Button_CancelAndClose)
    }
  </form>
</div>
